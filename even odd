a=int(input())
for i in range(1,a+1):
    if (i%2!=0):
        print("*"*i)
    else:
          print("#"*i)
b=int(input())
for  i in range(1,b+1):
    for j in range(1,i+1):
        print(j ,end=" ")
    print(" ")
c=int(input())
for i in range(c,0.-1):
    for j in range(i,i+j):
        print(j ,end=" ")
    print(" ")
d=int(input())
for i in range(1,d+1):
    for j in range(1,i+1):
        print(i,end=" ")
    print(" ")    
